{
    "mainClass": "com.thelogicmaster.test.SwitchLauncher",
    "reflective": [
        "com.badlogic.gdx.tests.JsonTest$*",
        "com.badlogic.gdx.tests.AnnotationTest$*",
        "com.badlogic.gdx.tests.ReflectionCorrectnessTest$*",
        "com.badlogic.gdx.tests.AbstractTestWrapper",
        "com.badlogic.gdx.utils.*Array",
        "com.badlogic.gdx.utils.*Set",
        "com.badlogic.gdx.utils.*Map"
    ],
    "jniClasses": ["com.badlogic.gdx.physics.bullet.**JNI"],
    "useStackTraces": true,
    "useLineNumbers": true,
    "useValueChecks": true,
    "useStackCookies": true,
    "useLeakCheck": false
}
